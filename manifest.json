{
  "manifest_version": 2,

  "name": "First extension",
  "description": "idk",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "click here"
  },
  "permissions": [
    "activeTab",
    "storage",
    "*://*.quotesondesign.com/*",
    "*://*.source.unsplash.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}